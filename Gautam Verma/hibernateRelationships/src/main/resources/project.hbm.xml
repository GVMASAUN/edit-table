<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.codeinsight.exercise.hibernateRelationships.model.Project"
		table="project">
		<id name="projectId" column="projectId">
			<generator class="native" />
		</id>
		<property name="name" column="name" />
		<set name="employees" table="employee_project" inverse="false"
			lazy="false" fetch="join">
			<key column="projectId"></key>
			<many-to-many column="id"
				class="com.codeinsight.exercise.hibernateRelationships.model.Employee"></many-to-many>
		</set>
	</class>
</hibernate-mapping>